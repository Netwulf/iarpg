{
  "name": "@aios-fullstack/core",
  "version": "4.31.0",
  "description": "AIOS-FullStack Core - Meta-agent and component generation system",
  "main": "index.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "bin": {
    "aios-core": "bin/aios-core.js"
  },
  "files": [
    "lib/",
    "bin/",
    "templates/",
    "utils/",
    "tasks/",
    "docs/",
    "elicitation/",
    "index.js",
    "index.esm.js",
    "index.d.ts",
    "README.md",
    "LICENSE"
